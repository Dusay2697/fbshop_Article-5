<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.spring.fbshop.front.user.dto.UserDto" alias="userDto"/>	
		<typeAlias type="com.spring.fbshop.front.review.dto.ReviewDto" alias="reviewDto"/>
		<typeAlias type="com.spring.fbshop.admin.product.dto.ProductDto" alias="productDto"/>
	</typeAliases>		
	
	


	<mappers>
		<mapper resource="com/spring/fbshop/mybatis/UserMapper.xml"/>
		<mapper resource="com/spring/fbshop/mybatis/ReviewMapper.xml"/>
		<mapper resource="com/spring/fbshop/mybatis/TeamMapper.xml"/>
		<mapper resource="com/spring/fbshop/mybatis/ProductMapper.xml"/>
	</mappers>
</configuration>